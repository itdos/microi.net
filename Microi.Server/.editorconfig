[*.cs]

# CS4014: 由于此调用不会等待，因此在调用完成前将继续执行当前方法
dotnet_diagnostic.CS4014.severity = none

# CS1998: 异步方法缺少 "await" 运算符，将以同步方式运行
dotnet_diagnostic.CS1998.severity = none

# CS1591: 缺少对公共可见类型或成员的 XML 注释
dotnet_diagnostic.CS1591.severity = none

# CS0168: 声明了变量，但从未使用过
dotnet_diagnostic.CS0168.severity = none

# CS1587: XML 注释没有放在有效语言元素上
dotnet_diagnostic.CS1587.severity = none

# CS1572: XML 注释中有 param 标记，但是没有该名称的参数
dotnet_diagnostic.CS1572.severity = none

# CS0162: 检测到无法访问的代码
dotnet_diagnostic.CS0162.severity = none

# CS8618: 在退出构造函数时，不可为 null 的字段必须包含非 null 值。请考虑添加 "required" 修饰符或声明为可为 null。
dotnet_diagnostic.CS8618.severity = none

# CS8603: 可能返回 null 引用。
dotnet_diagnostic.CS8603.severity = none

# CS8625: 无法将 null 字面量转换为非 null 的引用类型。
dotnet_diagnostic.CS8625.severity = none

# CS1573: 参数在 XML 注释中没有匹配的 param 标记(但其他参数有)
dotnet_diagnostic.CS1573.severity = none

# CS8600: 将 null 字面量或可能为 null 的值转换为非 null 类型。
dotnet_diagnostic.CS8600.severity = none

# CS8601: 可能的 null 引用赋值。
dotnet_diagnostic.CS8601.severity = none

# CS8602: 解引用可能出现空引用。
dotnet_diagnostic.CS8602.severity = none

# CS8604: 可能的 null 引用参数。
dotnet_diagnostic.CS8604.severity = none

# CS8619: 值中引用类型的为 Null 性与目标类型不匹配。
dotnet_diagnostic.CS8619.severity = none

# CS8765: 参数类型的为 Null 性与重写成员不匹配。
dotnet_diagnostic.CS8765.severity = none

# CS0219: 变量已赋值，但从未使用其值
dotnet_diagnostic.CS0219.severity = none

# CS1570: XML 注释出现 XML 格式错误
dotnet_diagnostic.CS1570.severity = none

# CS1711: XML 注释中有 typeparam 标记，但没有该名称的类型参数
dotnet_diagnostic.CS1711.severity = none

# CS0618: 成员已过时（普通过时警告）
dotnet_diagnostic.CS0618.severity = suggestion

# ASP0019: 建议使用 IHeaderDictionary.Append
dotnet_diagnostic.ASP0019.severity = suggestion
